@model digibox.services.Models.ReplenishModel
@{
    ViewBag.Title = "Open";
    var user = ViewData["user"] as string;
}

<div class="row">
    <div class="col-md-12">
        <div class="card card-default">
            <div class="card-header">
                <h3 class="card-title">@ViewBag.Title</h3>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
                @Html.HiddenFor(x => x.id)
                <div class="form-group row">
                    @Html.LabelFor(x => x.no, new { @class = "col-sm-2 col-form-label" })
                    <div class="col-sm-4">
                        <label class = "form-control">@Model.no</label>
                    </div>
                    @Html.LabelFor(x => x.indate, new { @class = "col-sm-2 col-form-label" })
                <div class="col-sm-4">
                    <label class="form-control">@String.Format("{0:dd MMM yyyy}",Model.indate)</label>
                </div>
                </div>
                <div class="form-group row">
                    <label class="col-form-label col-sm-2">Collector</label>
                    <div class="col-sm-10">
                        <label class="form-control">@user</label>
                    </div>
                </div>
                <div class="form-group row">
                    <table class="table" id="tblDetail">
                        <thead>
                            <tr>
                                <th>
                                    RFID No.
                                </th>
                                <th>
                                    Part. No
                                </th>
                                <th>
                                    Material
                                </th>
                                <th>
                                    Amount
                                </th>
                                <th>
                                    Unit
                                </th>
                                <th>
                                    <a href="~/Replenish/PrintAll/@Model.id">
                                        <i class="fa fa-print"></i>
                                    </a>
                                </th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
            <!-- /.card-body -->
        </div>
    </div>
</div>

@section scripts{

    <script src="~/Scripts/Replenish/Open.js"></script>

}
